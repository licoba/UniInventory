{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"app-plus": {
					"titleNView": false
				} // 隐藏导航栏
			}
		}, {
			"path": "pages/main/main",
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}
		}, {
			"path": "pages/admin/admin",
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}
		}
	    ,{
            "path" : "pages/admin/user",
            "style": {
            	"app-plus": {
            		"titleNView": false
            	}
            }
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": { //easycom组件模式（无需手动引入，开发工具自带智能感知）
		"autoscan": true,
		"custom": {
			"tui-(.*)": "@/components/ThorUI-uni-components/thorui/tui-$1/tui-$1.vue"
		}
	},
	"condition": { //模式配置，仅开发期间生效
		"current": 1, //当前激活的模式(list 的索引项)
		"list": [{
			"name": "管理员界面", //模式名称
			"path": "/pages/admin/admin", //启动页面，必选
			"query": "" //启动参数，在页面的onLoad函数里面得到
		}, {
			"name": "普通用户界面", //模式名称
			"path": "/pages/main/main", //启动页面，必选
			"query": "" //启动参数，在页面的onLoad函数里面得到
		}]
	}
}
